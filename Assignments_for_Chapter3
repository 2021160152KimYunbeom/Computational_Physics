        "#Compare the ratio between proton and electron with 6pi^5\n",
        "import numpy as np\n",
        "\n",
        "Mp = 938.272046;\n",
        "Me = 0.510998926;\n",
        "r = Mp/Me;\n",
        "print(f'r = (Mass of the proton)/(Mass of the electron) = {r}')\n",
        "print(f'6pi^5 = {6*np.pi**5}')\n",
        "\n",
        "if r > 6*(np.pi)**5:\n",
        "  print(\"(Mass of the proton)/(Mass of the electron) > 6pi^5\")\n",
        "else:\n",
        "  print(\"(Mass of the proton)/(Mass of the electron) < 6pi^5\")"
      ]
